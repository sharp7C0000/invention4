        icon="arrow-back"></paper-icon-button>
      <span flex>Title</span>
      <paper-icon-button id="searchbutton" 
        icon="search"></paper-icon-button>
    </core-toolbar>

  <script src="components/requirejs/require.js" data-main="js/index.js"></script>

<% include footer %>